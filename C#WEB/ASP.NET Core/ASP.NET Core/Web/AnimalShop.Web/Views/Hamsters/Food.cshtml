@model AnimalShop.Web.ViewModels.Food.FoodListeningViewModel

@{
    this.ViewData["Title"] = "Hamsters Food";
}

<div>
    <p>
        <strong>@Model.Count product(s) found</strong>
    </p>
</div>

<div class="row">
    @foreach (var food in Model.Food)
    {
        <partial name="../Shared/_FoodPartialView.cshtml" model="food" />
    }
</div>

